{
  "name": "my-app",
  "productName": "my-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "dist/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:main": "vite build --config vite.main.config.ts --watch",
    "dev:preload": "vite build --config vite.preload.config.ts --watch",
    "dev:renderer": "vite --config vite.renderer.config.mjs",
    "dev:electron": "nodemon --watch dist/main --delay 1000ms --exec \"electron dist/main/main.js\"",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "vite build --config vite.main.config.ts",
    "build:preload": "vite build --config vite.preload.config.ts",
    "build:renderer": "vite build --config vite.renderer.config.mjs",
    "start": "npm run build && electron dist/main/main.js",
    "dist": "npm run build && NODE_ENV=production electron-builder",
    "dist:mac": "npm run build && NODE_ENV=production electron-builder --mac",
    "dist:win": "npm run build && NODE_ENV=production electron-builder --win",
    "dist:linux": "npm run build && NODE_ENV=production electron-builder --linux",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,css}\"",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts"
  },
  "keywords": [],
  "author": {
    "name": "Joe Czubiak",
    "url": "https://joeczubiak.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.5",
    "electron": "38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^6.3.6"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.4",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "@tanstack/vue-query": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "electron-squirrel-startup": "^1.0.1",
    "hono": "^4.9.8",
    "lucide-vue-next": "^0.544.0",
    "reka-ui": "^2.5.1",
    "shadcn-vue": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  }
}
